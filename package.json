{
  "name": "prestyled",
  "version": "0.6.0",
  "description": "React components for styling web projects, based upon Theme UI",
  "keywords": [
    "react",
    "theme-ui",
    "emotion",
    "design-system",
    "styleguide",
    "styleguidist"
  ],
  "author": "Andreas Adam <andreas.sahle@gmail.com> (https://pixelmord.de)",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "module": "dist/prestyled.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/pixelmord/prestyled",
  "scripts": {
    "start": "tsdx watch",
    "dev": "yarn start",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "postbuild": "yarn styleguide:build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "presem": "yarn build",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsdx lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@theme-ui/color": "^0.3.1",
    "@theme-ui/preset-tailwind": "^0.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "theme-ui": "^0.3.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/babel__core": "^7.1.7",
    "@types/eslint": "^6.8.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/prettier": "^2.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/semantic-release": "^17.0.1",
    "@types/theme-ui": "^0.3.2",
    "@types/theme-ui__components": "^0.2.5",
    "@types/webpack": "^4.41.13",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-docgen-typescript": "^1.16.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-styleguidist": "^11.0.6",
    "semantic-release": "^17.0.8",
    "solidarity": "^3.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "dist",
            "docs"
          ],
          "message": "chore(release): ${nextRelease.version} [skip-ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
