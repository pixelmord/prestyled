{
  "name": "prestyled",
  "version": "0.3.10",
  "description": "components for styling web projects",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/pixelmord/prestyled",
  "scripts": {
    "lint": "eslint './src/**/*' --fix",
    "build": "pika-pack build",
    "storybook": "start-storybook -p 9009 -c .storybook",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "preversion": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Andreas Sahle <andreas.sahle@gmail.com> (https://pixelmord.github.io)",
  "license": "MIT",
  "dependencies": {
    "@emotion/cache": "^10.0.15",
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "@storybook/addon-a11y": "^5.1.10",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-info": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addon-notes": "^5.0.1",
    "@storybook/addon-storysource": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@storybook/theming": "^5.1.10",
    "@styled-system/theme-get": "^5.0.18",
    "emotion-theming": "^10.0.14",
    "html-loader": "^0.5.5",
    "markdown-loader": "^5.1.0",
    "polished": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "storybook-addon-emotion-theme": "^2.1.0",
    "storybook-readme": "^5.0.8",
    "styled-system": "^5.0.18"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@pika/pack": "^0.4.0",
    "@pika/plugin-build-node": "^0.5.1",
    "@pika/plugin-build-web": "^0.5.1",
    "@pika/plugin-standard-pkg": "^0.5.1",
    "@pika/plugin-ts-standard-pkg": "^0.5.1",
    "@pika/types": "^0.5.1",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/babel__core": "^7.1.2",
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "@types/prettier": "^1.18.1",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/storybook-readme": "^5.0.3",
    "@types/storybook__addon-a11y": "^5.0.0",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-system": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.1.0",
    "solidarity": "^2.3.1",
    "ts-jest": "^24.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
