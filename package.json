{
  "name": "prestyled",
  "version": "0.0.0-semantically-released",
  "description": "components for styling web projects",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/pixelmord/prestyled",
  "scripts": {
    "lint": "eslint './src/**/*' --fix",
    "build": "yarn pika build",
    "storybook": "start-storybook -p 9009 -c .storybook",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "presem": "yarn build",
    "sem": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Andreas Adam <andreas.sahle@gmail.com> (https://pixelmord.de)",
  "license": "MIT",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "theme-ui": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-standard-pkg": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@pika/types": "^0.8.1",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^7.0.0",
    "@semantic-release/git": "^8.0.0",
    "@semantic-release/github": "^6.0.1",
    "@semantic-release/npm": "^6.0.0",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "@types/babel__core": "^7.1.3",
    "@types/eslint": "^6.1.4",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/theme-ui": "^0.2.7",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.2.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-styleguidist": "^10.6.1",
    "semantic-release": "^16.0.3",
    "solidarity": "^2.3.1",
    "ts-jest": "^24.3.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
