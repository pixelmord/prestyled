{
  "name": "prestyled",
  "version": "0.3.5",
  "description": "components for styling web projects",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": "https://github.com/pixelmord/prestyled",
  "scripts": {
    "build": "tsc",
    "storybook": "start-storybook -p 9009 -c .storybook",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "preversion": "yarn build"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint -c tslint.json --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Andreas Sahle <andreas.sahle@gmail.com> (https://pixelmord.github.io)",
  "license": "MIT",
  "dependencies": {
    "@emotion/cache": "^10.0.7",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@storybook/addon-a11y": "^5.0.1",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-info": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addon-notes": "^5.0.1",
    "@storybook/addon-storysource": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "@storybook/theming": "^5.0.1",
    "emotion-theming": "^10.0.7",
    "html-loader": "^0.5.5",
    "markdown-loader": "^5.0.0",
    "polished": "^3.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "storybook-addon-emotion-theme": "^2.1.0",
    "storybook-readme": "^5.0.1",
    "styled-system": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/babel__core": "^7.1.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.0",
    "@types/prettier": "^1.16.1",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/storybook-readme": "^4.0.0",
    "@types/storybook__addon-a11y": "^3.3.1",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-info": "^4.1.0",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__addon-notes": "^4.0.2",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-system": "^3.2.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-docgen-typescript-loader": "^3.0.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "@types/react": "^16.8.7"
  }
}
