{
  "name": "prestyled",
  "version": "0.10.0",
  "description": "React components for styling web projects, based upon Theme UI",
  "keywords": [
    "react",
    "theme-ui",
    "emotion",
    "design-system",
    "styleguide",
    "styleguidist"
  ],
  "author": "Andreas Adam <andreas.sahle@gmail.com> (https://pixelmord.de)",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "module": "dist/prestyled.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/pixelmord/prestyled",
  "scripts": {
    "start": "tsdx watch",
    "dev": "yarn start",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "postbuild": "yarn styleguide:build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "presem": "yarn build",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsdx lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/styled": "^10.0.27",
    "@theme-ui/color": "^0.3.1",
    "@theme-ui/preset-tailwind": "^0.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-system": "^5.1.5",
    "theme-ui": "^0.3.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "resolutions": {
    "@babel/core": "7.11.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/babel__core": "^7.1.7",
    "@types/eslint": "^7.2.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/prettier": "^2.0.2",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "@types/semantic-release": "^17.1.0",
    "@types/styled-system": "^5.1.10",
    "@types/theme-ui": "^0.3.6",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.12",
    "prettier": "^2.1.0",
    "react-docgen-typescript": "^1.20.3",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-styleguidist": "^11.0.6",
    "semantic-release": "^19.0.3",
    "solidarity": "^3.0.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.3",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "dist",
            "docs"
          ],
          "message": "chore(release): ${nextRelease.version} [skip-ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
