{
  "name": "prestyled",
  "version": "0.3.14",
  "description": "components for styling web projects",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/pixelmord/prestyled",
  "scripts": {
    "lint": "eslint './src/**/*' --fix",
    "build": "pika build",
    "publish": "pika publish",
    "storybook": "start-storybook -p 9009 -c .storybook",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "preversion": "yarn build",
    "version": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Andreas Sahle <andreas.sahle@gmail.com> (https://pixelmord.github.io)",
  "license": "MIT",
  "dependencies": {
    "@emotion/cache": "^10.0.27",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@storybook/addon-a11y": "^5.3.6",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-info": "^5.3.6",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addon-notes": "^5.3.6",
    "@storybook/addon-storysource": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "@storybook/theming": "^5.3.6",
    "@styled-system/theme-get": "^5.1.2",
    "emotion-theming": "^10.0.27",
    "html-loader": "^0.5.5",
    "markdown-loader": "^5.1.0",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "storybook-addon-emotion-theme": "^2.1.1",
    "storybook-readme": "^5.0.8",
    "styled-system": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-standard-pkg": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@pika/types": "^0.8.1",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/babel__core": "^7.1.3",
    "@types/eslint": "^6.1.3",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/styled-system": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "solidarity": "^2.3.1",
    "ts-jest": "^24.3.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
